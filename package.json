{
  "name": "userscript-with-typescript-template",
  "version": "0.0.0",
  "scripts": {
    "build": "npm run -s lint && npm run -s build-plain && npm run -s build-minified",
    "lint": "tslint src/**/*.ts{,x}",
    "build-plain": "webpack --mode=none && concat-cli -f src/UserScriptDeclaration.js dist/out.js -o dist/DemoScript.user.js",
    "build-minified": "webpack --mode=production && concat-cli -f src/UserScriptDeclaration.js dist/out.js -o dist/DemoScript.min.user.js"
  },
  "author": "Robert Rudman <rjrudman@gmail.com> (https://github.com/rjrudman)",
  "license": "ISC",
  "devDependencies": {
    "awesome-typescript-loader": "5.2.1",
    "concat-cli": "4.0.0",
    "ts-loader": "5.3.0",
    "tslib": "1.9.3",
    "tslint": "5.11.0",
    "typescript": "3.1.6",
    "webpack": "4.26.0",
    "webpack-cli": "^3.1.2"
  }
}
